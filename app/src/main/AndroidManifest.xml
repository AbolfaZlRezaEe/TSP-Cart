<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abproject.tsp_cart">

    <application
        android:name="com.abproject.TSPApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TSPCart">
        <activity android:name=".view.cart.CartActivity"></activity>
        <activity android:name=".view.productdetail.ProductDetailActivity" />
        <activity android:name=".view.addproduct.AddProductActivity" />
        <activity android:name=".view.user.UserActivity" />
        <activity android:name=".view.auth.AuthActivity" />
        <activity android:name=".view.admin.AdminActivity" />
        <activity android:name=".view.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>